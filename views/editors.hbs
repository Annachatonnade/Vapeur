{{> header}}

<main>
  <h2>Editor Management</h2>
  <form action="/editors" method="POST">
    <input type="text" name="name" placeholder="Editor Name" required />
    <button type="submit">Add Editor</button>
  </form>
  <ul>
    {{#each editors}}
      <li>{{this.name}}</li>
    {{/each}}
  </ul>
</main>

{{> footer}}
